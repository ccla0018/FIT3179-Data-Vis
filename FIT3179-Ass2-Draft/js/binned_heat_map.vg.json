{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/ccla0018/FIT3179-Data-Vis/main/FIT3179-Ass2-Draft/data/binned_heat_map.csv"
  },
  "transform": [
    {
      "filter": {
        "and": [
          { "field": "ef_money", "valid": true },
          { "field": "ef_legal", "valid": true }
        ]
      }
    }
  ],
  "mark": "rect",
  "width": 325,
  "height": 300,
  "encoding": {
    "x": {
      "bin": { "maxbins": 20 },
      "field": "ef_money",
      "type": "quantitative"
    },
    "y": {
      "bin": { "maxbins": 20 },
      "field": "ef_legal",
      "type": "quantitative"
    },
    "color": {
      "aggregate": "count",
      "type": "quantitative",
      "scale": {
        "scheme": "tealblues"
      }
    },
    "tooltip": [
      { "field": "ef_legal_range", "title": "EF Legal Range" },
      { "field": "ef_money_range", "title": "EF Money Range" },
      { "aggregate": "count", "type": "quantitative", "title": "Count" }
    ]
  },
  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}
